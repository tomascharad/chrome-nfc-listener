{
  "manifest_version": 2,

  "name": "Chrome Nfc Listener",
  "description": "Listens to Nfc serial monitor messages",
  "version": "1.1",

  "icons": { "128": "DispatchApp.png" },

  "background": {
    "persistent": true,
    "scripts": ["bg.js"]
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScript.js"]
    }
  ]
}